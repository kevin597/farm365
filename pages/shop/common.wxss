.flex-row {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}

.flex-top {
  align-items: flex-start;
}

.flex-full {
  flex: 1;
  overflow: hidden;
}

.font-size-22 {
  font-size: 22rpx;
}

.font-size-24 {
  font-size: 24rpx;
}

.font-size-26 {
  font-size: 26rpx;
}

.font-size-28 {
  font-size: 28rpx;
}

.font-size-30 {
  font-size: 30rpx;
}

.font-size-32 {
  font-size: 32rpx;
}

.font-color-red {
  color: #e90000;
}

.text-a-r {
  text-align: right;
}

.product_list_ul {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  align-content: flex-start;
  background-color: #f7f7f7;
  padding: 10rpx 0;
}

.product_list_li {
  flex: 0 0 50%;
  height: 490rpx;
  box-sizing: border-box;
  overflow: hidden;
}

.product_list_li:nth-child(2n) {
  padding-left: 5rpx;
}

.pro_list_img {
  width: 370rpx;
  height: 370rpx;
  display: block;
}

.pro_list_title {
  background-color: rgba(0, 0, 0, 0.5);
  height: 70rpx;
  width: 370rpx;
  margin-top: -70rpx;
  font-size: 30rpx;
  color: #fff;
  padding-left: 10rpx;
  line-height: 70rpx;
  box-sizing: border-box;
}

.pro_list_info {
  height: 110rpx;
  background-color: #fff;
  padding: 10rpx 30rpx;
  width: 370rpx;
  box-sizing: border-box;
}

.pro_list_info text {
  display: block;
  float: left;
  line-height: 50rpx;
}

.pro_list_info .width40 {
  width: 40%;
}

.pro_list_info .width60 {
  width: 60%;
}
